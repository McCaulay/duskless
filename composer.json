{
    "name": "mccaulay/duskless",
    "description": "Laravel Duskless provides browser automation.",
    "keywords": ["laravel", "webdriver"],
    "license": "MIT",
    "require": {
        "php": "^7.2|^8.0",
        "ext-json": "*",
        "ext-zip": "*",
        "php-webdriver/webdriver": "^1.9.0",
        "nesbot/carbon": "^2.0",
        "illuminate/console": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "symfony/console": "^4.3|^5.0",
        "symfony/finder": "^4.3|^5.0",
        "symfony/process": "^4.3|^5.0",
        "vlucas/phpdotenv": "^3.0|^4.0|^5.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "McCaulay\\Duskless\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "McCaulay\\Duskless\\DusklessServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
